{
  "name": "maritime-vessel-management-system",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node --experimental-specifier-resolution=node --loader ts-node/esm server/server.ts",
    "server:dev": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm server/server.ts",
    "start": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "db:populate": "node server/scripts/runPopulate.js",
    "db:seed": "npx ts-node --esm --experimental-specifier-resolution=node server/scripts/seedData.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@types/leaflet": "^1.9.17",
    "axios": "^1.8.4",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "mongoose": "^8.13.2",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
